/* 字数限制 */
.limit{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.limit2{
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
}